<h3><%= @stores.count %> Total Stores</h3>

<% @stores.each do |store| %>
  <ul class="store_info">
    <p class="long_name"><%= store.long_name %></p>
    <li class="city">City: <%= store.city %></li>
    <li class="distance">Distance: <%= store.distance %> mi</li>
    <li class="phone">Phone: <%= store.phone %></li>
    <li class="store_type">Type: <%= store.store_type %></li>
  </ul>
<% end %>
